{
	"name": "figma-icon-font-generator",
	"version": "1.0.0",
	"description": "Figma icon font generator plugin",
	"license": "ISC",
	"author": {
		"name": "Victor Assis",
		"url": "https://github.com/victor-assis"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/victor-assis/figma-icon-font-generator"
	},
  "engines": {
    "node": ">=16.0.0"
  },
	"keywords": [
		"SVG",
		"icons",
		"creation",
		"fonts",
		"figma"
	],
	"scripts": {
		"start": "webpack --mode=development --watch",
		"build": "webpack --mode=production",
		"eslint": "eslint src/**/*.{js,ts,tsx}",
		"eslint:fix": "eslint src/**/*.{js,ts,tsx} --fix",
		"prettier:format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,json}"
	},
	"dependencies": {
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@mui/icons-material": "^5.14.3",
		"@mui/lab": "5.0.0-alpha.138",
		"@mui/material": "^5.14.3",
		"@resvg/resvg-js": "^2.6.0",
		"cheerio": "^0.22.0",
		"file-saver": "^2.0.5",
		"jszip": "^3.10.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"styled-components": "^6.0.7",
		"svg2ttf": "^6.0.3",
		"svgicons2svgfont": "^12.0.0",
		"ttf2eot": "^2.0.0",
		"ttf2woff": "^2.0.2",
		"ttf2woff2": "^3.0.0",
		"xmldom": "^0.6.0"
	},
	"devDependencies": {
		"@figma/plugin-typings": "^1.72.0",
		"@types/file-saver": "^2.0.5",
		"@types/node": "^18.7.23",
		"@types/react": "^18.2.18",
		"@types/react-dom": "^18.2.7",
		"@types/svg2ttf": "^5.0.1",
		"@types/svgicons2svgfont": "^10.0.1",
		"@types/ttf2eot": "^2.0.0",
		"@types/ttf2woff": "^2.0.2",
		"@types/ttf2woff2": "^2.0.0",
		"@typescript-eslint/eslint-plugin": "^6.2.1",
		"css-loader": "^5.0.1",
		"eslint": "^8.46.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-config-standard-with-typescript": "^37.0.0",
		"eslint-plugin-import": "^2.28.0",
		"eslint-plugin-n": "^16.0.1",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.33.1",
		"html-webpack-inline-source-plugin": "^0.0.10",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.3",
		"oslllo-potrace": "^2.0.1",
		"oslllo-svg2": "^2.0.2",
		"prettier": "3.0.1",
		"sass": "^1.64.2",
		"sass-loader": "^10.3.1",
		"style-loader": "^2.0.0",
		"ts-loader": "^8.0.11",
		"typescript": "^5.1.6",
		"url-loader": "^4.1.1",
		"webpack": "^4.41.4",
		"webpack-cli": "^3.3.6"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,css,json}": [
			"prettier --write",
			"git add"
		]
	}
}
