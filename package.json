{
	"name": "figma-icon-font-generator",
	"version": "1.0.0",
	"description": "Figma icon font generator plugin",
	"license": "ISC",
	"author": {
		"name": "Victor Assis",
		"url": "https://github.com/victor-assis"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/victor-assis/figma-icon-font-generator"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"keywords": [
		"SVG",
		"icons",
		"creation",
		"fonts",
		"figma"
	],
	"scripts": {
		"start": "webpack --mode=development --watch",
		"build": "webpack --mode=production",
		"eslint": "eslint src/**/*.{js,ts,tsx}",
		"eslint:fix": "eslint src/**/*.{js,ts,tsx} --fix",
		"prettier:format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,json}",
		"fix": "oslllo-svg-fixer -s ./svgs -d ./fixed"
	},
	"dependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.0",
		"@mui/icons-material": "^7.1.1",
		"@mui/lab": "7.0.0-beta.13",
		"@mui/material": "^7.1.1",
		"cheerio": "1.0.0",
		"file-saver": "^2.0.5",
		"jszip": "^3.10.1",
		"oslllo-svg-fixer": "^5.0.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"styled-components": "^6.1.18",
		"svg2ttf": "^6.0.3",
		"svgicons2svgfont": "^15.0.1",
		"ttf2eot": "^3.1.0",
		"ttf2woff": "^3.0.0",
		"ttf2woff2": "^3.0.0",
		"webfonts-generator": "^0.4.0",
		"xmldom": "^0.6.0"
	},
	"devDependencies": {
		"@figma/plugin-typings": "^1.113.0",
		"@types/file-saver": "^2.0.7",
		"@types/node": "^22.15.30",
		"@types/react": "^19.1.6",
		"@types/react-dom": "^19.1.6",
		"@types/svg2ttf": "^5.0.3",
		"@types/ttf2eot": "^2.0.2",
		"@types/ttf2woff": "^2.0.4",
		"@types/ttf2woff2": "^2.0.0",
		"@typescript-eslint/eslint-plugin": "^8.33.1",
		"assert": "^2.1.0",
		"browserify-zlib": "^0.2.0",
		"crypto-browserify": "^3.12.1",
		"css-loader": "^7.1.2",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-config-standard-with-typescript": "^43.0.1",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-n": "^17.19.0",
		"eslint-plugin-promise": "^7.2.1",
		"eslint-plugin-react": "^7.37.5",
		"html-webpack-plugin": "^5.6.3",
		"https-browserify": "^1.0.0",
		"os-browserify": "^0.3.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.0",
		"node-loader": "^2.1.0",
		"path-browserify": "^1.0.1",
		"prettier": "3.5.3",
		"process": "^0.11.10",
		"querystring-es3": "^0.2.1",
		"react-dev-utils": "^12.0.1",
		"sass": "^1.89.1",
		"sass-loader": "^16.0.5",
		"stream-browserify": "^3.0.0",
		"stream-http": "^3.2.0",
		"string_decoder": "^1.3.0",
		"style-loader": "^4.0.0",
		"ts-loader": "^9.5.2",
		"typescript": "^5.8.3",
		"url": "^0.11.4",
		"url-loader": "^4.1.1",
		"util": "^0.12.5",
		"vm-browserify": "^1.1.2",
		"webpack": "^5.99.9",
		"webpack-cli": "^6.0.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,css,json}": [
			"prettier --write",
			"git add"
		]
	},
	"packageManager": "yarn@4.9.2"
}