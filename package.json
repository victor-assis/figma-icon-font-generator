{
	"name": "figma-icon-font-generator",
	"version": "1.0.0",
	"description": "Figma icon font generator plugin",
	"license": "ISC",
	"author": {
		"name": "Victor Assis",
		"url": "https://github.com/victor-assis"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/victor-assis/figma-icon-font-generator"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"keywords": [
		"SVG",
		"icons",
		"creation",
		"fonts",
		"figma"
	],
	"scripts": {
		"start": "webpack --mode=development --watch",
		"build": "webpack --mode=production",
		"eslint": "eslint src/**/*.{js,ts,tsx}",
		"eslint:fix": "eslint src/**/*.{js,ts,tsx} --fix",
		"prettier:format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,json}",
		"fix": "oslllo-svg-fixer -s ./svgs -d ./fixed"
	},
	"dependencies": {
		"@emotion/react": "^11.11.4",
		"@emotion/styled": "^11.11.5",
		"@mui/icons-material": "^5.15.16",
		"@mui/lab": "5.0.0-alpha.170",
		"@mui/material": "^5.15.16",
		"cheerio": "1.0.0-rc.12",
		"file-saver": "^2.0.5",
		"jszip": "^3.10.1",
		"oslllo-svg-fixer": "^4.0.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"styled-components": "^6.1.9",
		"svg2ttf": "^6.0.3",
		"svgicons2svgfont": "^12.0.0",
		"ttf2eot": "^3.1.0",
		"ttf2woff": "^3.0.0",
		"ttf2woff2": "^4.0.5",
		"webfonts-generator": "^0.4.0",
		"xmldom": "^0.6.0"
	},
	"devDependencies": {
		"@figma/plugin-typings": "^1.92.0",
		"@types/file-saver": "^2.0.7",
		"@types/node": "^20.12.8",
		"@types/react": "^18.3.1",
		"@types/react-dom": "^18.3.0",
		"@types/svg2ttf": "^5.0.3",
		"@types/svgicons2svgfont": "^10.0.5",
		"@types/ttf2eot": "^2.0.2",
		"@types/ttf2woff": "^2.0.4",
		"@types/ttf2woff2": "^2.0.2",
		"@typescript-eslint/eslint-plugin": "^7.8.0",
		"assert": "^2.1.0",
		"browserify-zlib": "^0.2.0",
		"crypto-browserify": "^3.12.0",
		"css-loader": "^7.1.1",
		"eslint": "^9.2.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-standard-with-typescript": "^43.0.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-n": "^17.4.0",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.34.1",
		"html-webpack-plugin": "^5.6.0",
		"https-browserify": "^1.0.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"node-loader": "^2.0.0",
		"path-browserify": "^1.0.1",
		"prettier": "3.2.5",
		"process": "^0.11.10",
		"querystring-es3": "^0.2.1",
		"react-dev-utils": "^12.0.1",
		"sass": "^1.76.0",
		"sass-loader": "^14.2.1",
		"stream-browserify": "^3.0.0",
		"stream-http": "^3.2.0",
		"string_decoder": "^1.3.0",
		"style-loader": "^4.0.0",
		"ts-loader": "^9.5.1",
		"typescript": "^5.4.5",
		"url": "^0.11.3",
		"url-loader": "^4.1.1",
		"util": "^0.12.5",
		"vm-browserify": "^1.1.2",
		"webpack": "^5.91.0",
		"webpack-cli": "^5.1.4"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,css,json}": [
			"prettier --write",
			"git add"
		]
	}
}
